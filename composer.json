{
    "name": "baytek/laravel-content",
    "description": "Content Provider",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Yvon Viger",
            "email": "yvon@baytek.ca"
        }
    ],
    "autoload": {
        "psr-4": {
            "Baytek\\Laravel\\": "src/"
        },
        "files": [
            "src/Content/localization.php",
            "src/Content/Support/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Baytek\\Laravel\\Content\\ServiceProvider"
            ]
        },
        "packages": {
            "baytek/laravel-menu": "~1.1.0",
            "baytek/laravel-statusbit": "~1.3.0",
            "baytek/laravel-user": "~1.1.0"
        }
    },
    "require": {
        "php": ">=5.6.4",
        "laracasts/flash":"^3.0.0",
        "laravelcollective/html":"^5.3.0"
    },
    "post-install-cmd": [
        "php artisan install:content"
    ],
    "minimum-stability": "dev"
}
